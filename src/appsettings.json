      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AWS": {
    "BucketName": "uploads-app-bucket",
    "Region": "eu-north-1",
    "Profile": "imageresizer"
  },
  "AzureBlobStorage": {
    "ConnectionString": "UseDevelopmentStorage=true",
    "ConnectionString2": "DefaultEndpointsProtocol=https;AccountName=vicappstorage;AccountKey=BbmWBxfC2oTUDx9ZL1g4AIhXvopVwbLuNmieHw+sVQDFs718ukK8TLCGpQoflGU1fEkOOfdjGg3L+AStmkwyzA==;EndpointSuffix=core.windows.net",
